<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
        <Authors>Terekhin Sergey</Authors>
        <Description>Wrapper for libjpeg-turbo library</Description>
        <AssemblyVersion>2.0.1.0</AssemblyVersion>
        <FileVersion>2.0.1.0</FileVersion>
        <Version>2.0.1</Version>
        <Product>Libjpeg-Turbo wrapper for .Net</Product>
        <PackageTags>TurboJpeg libjpeg-turbo</PackageTags>
        <Copyright>Copyright © 2018</Copyright>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>

    <ItemGroup>
        <None Update="turbojpeg.dll">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
    </ItemGroup>
</Project>